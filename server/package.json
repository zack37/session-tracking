{
  "name": "session-tracker-backend",
  "version": "0.1.0",
  "description": "Back-end services for workout-tracker",
  "main": "build/server.js",
  "author": "Zack Smith <zack.t.smith37@gmail.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "lint": "xo",
    "lint:fix": "npm run lint -- --fix",
    "start": "node build/server.js",
    "build": "rm -rf build && babel src --out-dir build --copy-files",
    "run:local": "BABEL_ENV=local NODE_ENV=local nodemon index.js",
    "test": "NODE_ENV=test mocha --recursive --require babel-core/register"
  },
  "lint-staged": {
    "*.js": ["npm run lint:fix --", "git add"],
    "*.{js,css,md}": ["prettier --write", "git add"],
    "*.json": ["prettier --write --print-width=120", "git add"]
  },
  "xo": {
    "prettier": true,
    "envs": ["node", "mocha"],
    "plugins": ["node", "unicorn", "import"],
    "ignore": ["build"]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-console-groupify": "^0.0.8",
    "babel-plugin-transform-console-log-variable-names": "^0.0.3",
    "babel-preset-minify": "^0.4.3",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.12.1",
    "serverless": "^1.23.0",
    "xo": "^0.21.1"
  },
  "dependencies": {
    "apollo-server": "^2.0.0-beta.3",
    "apollo-server-hapi": "^2.0.0-beta.1",
    "bcrypt": "^2.0.1",
    "blipp": "^3.0.0",
    "chalk": "^2.1.0",
    "debug": "^3.1.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "graphql-tools": "^3.0.2",
    "hapi": "^17.5.1",
    "http-status-codes": "^1.3.0",
    "joi": "^13.3.0",
    "joi-date-extensions": "^1.1.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mod.js": "^2.0.0",
    "pg": "^7.4.3",
    "shortid": "^2.2.8",
    "source-map-support": "^0.5.0",
    "sql-query": "^0.1.26"
  }
}
